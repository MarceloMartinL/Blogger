<h1>All Tags</h1>

<ul>
  <% @tag.each do |tags| %>
    <li>
      <%= link_to tags, tag_path(tags) %>
      <%= link_to "delete", tag_path(@tag), method: :delete, data: {confirm: "Really delete the article?"} %>
    </li>
  <% end %>
</ul>

<%= link_to "Create a New Article", new_article_path, class: "new_article" %>
